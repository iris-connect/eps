{
  "entries" : [
    {
      "name": "op-1",
      "channels" : [
        {
          "type" : "grpc_server",
          "settings" : {
            "address" : "localhost:4444"
          }
        }
      ]
    },
    {
      "name": "ls-1",
      "services": [
        {
          "name": "locations",
          "permissions": [{"group": "LocationAdministrators", "rights" : ["call"]}],
          "methods": [
            {
              "name": "add",
              "params": [
                {
                  "name": "name",
                  "validators": [
                    {"type": "IsString", "parameters":{"MinLength": 1, "MaxLength": 100}},
                    {"type": "IsURL"}
                  ]
                },
                {
                  "name": "id",
                  "validators": [
                    {"type": "IsBytes", "parameters": {"Encoding": "base64", "MinLength": 16, "MaxLength": 16}}
                  ]
                }
              ]
            },
            {
              "name": "check",
              "permissions": [{"group": "HealthDepartments", "rights" : ["call"]}]
            }
          ]
        }
      ],
      "channels" : [
        {
          "type" : "grpc_server",
          "settings" : {
            "address" : "localhost:4445"
          }
        }
      ]
    },
    {
      "name": "hd-1",
      "channels" : [
        {
          "type" : "grpc_client"
        }
      ]
    },
    {
      "name": "private-proxy-1",
      "channels" : [
        {
          "type" : "grpc_client"
        }
      ]
    },
    {
      "name": "public-proxy-1",
      "channels" : [
        {
          "type" : "grpc_server",
          "settings" : {
            "address" : "localhost:5556"
          }
        },
        {
          "type" : "grpc_client"
        }
      ]
    }
  ]
}
